{
  "name": "presstify-plugins/optimizer",
  "description": "Extension PresstiFy d'optimisation.",
  "type": "presstify-plugin",
  "keywords": [
    "presstify",
    "plugins",
    "optimizer"
  ],
  "homepage": "https://github.com/milkcreation/presstify-optimizer.git",
  "time": "2021-09-14",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr",
      "role": "Developper"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "presstify/framework": ">=1.4",
    "rosell-dk/webp-convert": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "tiFy\\Plugins\\Optimizer\\": ""
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
